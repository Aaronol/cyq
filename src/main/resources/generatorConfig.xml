<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
    PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
    "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="DB2Tables" targetRuntime="MyBatis3">
        <plugin type="com.itfsw.mybatis.generator.plugins.ExampleTargetPlugin">
            <!-- 修改Example类生成到目标包下 -->
            <!--<property name="targetPackage" value="com.kyee.his.base.tmp.source.example"/>-->
            <property name="targetPackage" value="com.neo.entity"/>

        </plugin>
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!--数据库链接地址账号密码-->
        <jdbcConnection driverClass="oracle.jdbc.driver.OracleDriver"
            connectionURL="jdbc:oracle:thin:@127.0.0.1:1521:mytest"
            userId="ITISME" password="12345678">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!--生成Model类存放位置-->
        <!--<javaModelGenerator targetPackage="com.kyee.his.base.tmp.source.enity" targetProject="src/main/java">-->
        <javaModelGenerator targetPackage="com.neo.entity" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!--生成映射文件存放位置-->
        <!--<sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">-->
        <sqlMapGenerator targetPackage="mybatis.mapper" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!--生成Dao类存放位置-->
        <!-- 客户端代码，生成易于使用的针对Model对象和XML配置文件 的代码
                type="ANNOTATEDMAPPER",生成Java Model 和基于注解的Mapper对象
                type="MIXEDMAPPER",生成基于注解的Java Model 和相应的Mapper对象
                type="XMLMAPPER",生成SQLMap XML文件和独立的Mapper接口
        -->
        <!--<javaClientGenerator type="XMLMAPPER" targetPackage="com.kyee.his.base.tmp.source.mapper" targetProject="src/main/java">-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.neo.mapper"
            targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!--生成对应表及类名-->
        <!--<table tableName="PEXAM_MAINT"></table>-->
        <!--<table tableName="PEXAM_MANS"></table>-->
        <!--<table tableName="PEXAM_UNIT_GROUPT"></table>-->
        <table tableName="MWS"></table>
        <!--<table tableName="pexam_items_coms"></table>-->
        <!--<table tableName="PEXAM_ITEMSs"></table>-->
    </context>
</generatorConfiguration>